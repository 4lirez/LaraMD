{
    "name": "LaraMD",
    "version": "1.0.0",
    "description": "A chrome extension for displaying the laravel .md docs like laravel.com does",
    "content_scripts":[{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["file:///*/*md"]
    }],
    "icons": { 
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png"
    },
    "manifest_version": 2
}